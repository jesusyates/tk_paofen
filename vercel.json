{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' https://j56y.com; script-src 'self'; style-src 'self' 'unsafe-inline'; img-src * data:; connect-src https://j56y.com; frame-src 'none';",
          "override": true
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload",
          "override": true
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=(), payment=()",
          "override": true
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY",
          "override": true
        }
      ]
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
